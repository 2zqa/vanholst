{
  "@@locale": "en",
  "app_description": "Hardloopbegeleiding op elk niveau",
  "app_error_message": "An error occurred",
  "authentication_title": "Sign in",
  "authentication_login": "Email or username",
  "authentication_password": "Password",
  "authentication_sign_in": "Sign in",
  "logbook_not_provided": "not provided",
  "@logbook_not_provided": {
    "description": "The default value for when a logbook field is not provided. Dynamically capitalized."
  },
  "logbook_program_for": "Program for {date}",
  "@logbook_program_for": {
    "placeholders": {
      "date": {
        "type": "DateTime",
        "description": "The date the program is for",
        "format": "yMMMEd"
      }
    }
  },
  "logbook_no_program": "No program provided",
  "logbook_performance": "Performance",
  "logbook_circumstances": "Circumstances: {circumstances}",
  "@logbook_circumstances": {
    "placeholders": {
      "circumstances": {
        "type": "String",
        "example": "Surface: asphalt, weather: 10 degrees, footwear: trainers",
        "description": "The circumstances of the run. If not provided, the default value is taken from logbook_not_provided."
      }
    }
  },
  "logbook_link": "Link: {link}",
  "@logbook_link": {
    "placeholders": {
      "link": {
        "type": "String",
        "description": "The link to the run. If not provided, the default value is taken from logbook_not_provided."
      }
    }
  },
  "logbook_info_for_coach": "Info for coach",
  "logbook_sleep": "Sleep: {sleep}",
  "@logbook_sleep": {
    "placeholders": {
      "sleep": {
        "type": "String",
        "description": "The amount of sleep the night before the run. If not provided, the default value is taken from logbook_not_provided."
      }
    }
  },
  "logbook_timings": "Timings: {timings}",
  "@logbook_timings": {
    "placeholders": {
      "timings": {
        "type": "String",
        "description": "The timings of the run. If not provided, the default value is taken from logbook_not_provided."
      }
    }
  }
