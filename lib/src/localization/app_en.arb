{
  "@@locale": "en",
  "appDescription": "Hardloopbegeleiding op elk niveau",
  "appErrorMessage": "An error occurred",
  "authenticationTitle": "Sign in",
  "authenticationLogin": "Email or username",
  "authenticationPassword": "Password",
  "authenticationSignIn": "Sign in",
  "authenticationEmptyUsernameError": "Email can't be empty",
  "authenticationInvalidUsernameError": "Invalid email or username",
  "authenticationEmptyPasswordError": "Password can't be empty",
  "logbookNotProvided": "not provided",
  "@logbookNotProvided": {
    "description": "The default value for when a logbook field is not provided. Dynamically capitalized."
  },
  "logbookProgramFor": "Program for {date}",
  "@logbookProgramFor": {
    "placeholders": {
      "date": {
        "type": "DateTime",
        "description": "The date the program is for",
        "format": "yMMMEd"
      }
    }
  },
  "logbookNoProgram": "No program provided",
  "logbookPerformance": "Performance",
  "logbookCircumstances": "Circumstances: {circumstances}",
  "@logbookCircumstances": {
    "placeholders": {
      "circumstances": {
        "type": "String",
        "example": "Surface: asphalt, weather: 10 degrees, footwear: trainers",
        "description": "The circumstances of the run. If not provided, the default value is taken from logbookNotProvided."
      }
    }
  },
  "logbookLink": "Link: {link}",
  "@logbookLink": {
    "placeholders": {
      "link": {
        "type": "String",
        "description": "The link to the run. If not provided, the default value is taken from logbookNotProvided."
      }
    }
  },
  "logbookInfoForCoach": "Info for coach",
  "logbookSleep": "Sleep: {sleep}",
  "@logbookSleep": {
    "placeholders": {
      "sleep": {
        "type": "String",
        "description": "The amount of sleep the night before the run. If not provided, the default value is taken from logbookNotProvided."
      }
    }
  },
  "logbookTimings": "Timings: {timings}",
  "@logbookTimings": {
    "placeholders": {
      "timings": {
        "type": "String",
        "description": "The timings of the run. If not provided, the default value is taken from logbookNotProvided."
      }
    }
  }
}
